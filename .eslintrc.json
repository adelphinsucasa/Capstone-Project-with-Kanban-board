{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "linebreak-style": "off",
    "import/no-cycle": "off",
    "no-use-before-define": "off",
    "object-curly-newline": "off",
    "operator-linebreak": "off",
    "comma-dangle": "off",
    "import/extensions": [
      1,
      {
        "js": "always",
        "json": "always"
      }
    ]
  },
  "ignorePatterns": ["dist/", "build/"]
}
